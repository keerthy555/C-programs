#include<stdio.h>
#include<string.h>
int main()
{
	char s[100];
	gets(s);
	int i,count1=0,count2=0,count3=0;
	for(i=0;s[i]!='\0';i++)
	{
		if(isupper(s[i]))
		{
			count1++;
		}
		if(islower(s[i]))
		{
			count2++;
		}
		if(isdigit(s[i]))
		{
			count3++;
		}
	}
		printf("Count of uppercase characters: %d\n",count1);
		printf("Count of lowercase character: %d\n",count2);
		printf("Count of numbers: %d",count3);
	return 0;
	
	
}
